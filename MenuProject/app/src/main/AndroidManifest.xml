<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.frain.myapplication">
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ReviewFourthProject.activity.TodayNewsActivity"
            android:configChanges="orientation|screenSize">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".MainActivity" android:label="MainActivity">
            <!--意图过滤器-->
            <intent-filter >
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <data android:mimeType="image/png"/>
            </intent-filter>
        </activity>

        <activity android:name=".TwoActivity" android:label="TwoActivity">
            <!--意图过滤器-->
            <intent-filter>
                <action android:name="com.wanglaowu.haha" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".ReviewFourthProject.activity.NewsDetailsActivity"/>
        <activity android:name=".ReviewFourthProject.activity.ChannelManagerActivity"/>
    </application>

</manifest>